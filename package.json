  "scripts": {
    "watch": "npx -p typescript tsc -w",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:migrate:dev": "prisma migrate dev && npm run start:dev",
    "start:generate:migrate:dev": "prisma generate && prisma migrate dev && npm run start:dev",
    "start:debug": "nest start --debug --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:prod": "node dist/main",
    "start:migrate:prod": "prisma migrate deploy && npm run start:prod",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "NODE_ENV=test npx prisma generate && jest",
    "test:watch": "NODE_ENV=test npx prisma generate && jest --watch",
    "test:cov": "NODE_ENV=test npx prisma generate && jest --coverage",
    "test:debug": "NODE_ENV=test npx prisma generate && node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test npx prisma generate && jest --config ./test/jest-e2e.config.ts --detectOpenHandles",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate:dev": "npx prisma migrate dev"
  },
